game.Players.LocalPlayer.Backpack[ToolNameToDestroy]:Destroy()

local ToolNameToDestroy = "Shove"

game.Players.LocalPlayer.Backpack[ToolNameToDestroy]:Destroy()

local ToolNameToDestroy = "Uppercut"

